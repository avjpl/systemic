{
  "name": "systemic",
  "version": "3.3.7",
  "description": "A minimal dependency injection library for node",
  "main": "index.js",
  "scripts": {
    "qa": "npm run lint && npm run test",
    "lint": "eslint .",
    "test": "nyc --report html --reporter lcov --reporter text-summary node test"
  },
  "keywords": [
    "dependency",
    "injection",
    "context",
    "inversion of control",
    "graceful",
    "start up",
    "shutdown",
    "ioc",
    "boot"
  ],
  "author": "GuideSmiths Ltd",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-imperative": "^3.0.0",
    "eslint-plugin-imperative": "^3.0.0",
    "husky": "^4.2.3",
    "nyc": "^15.1.0",
    "zunit": "^3.0.0"
  },
  "dependencies": {
    "async": "^3.2.0",
    "chance": "^1.1.4",
    "debug": "^4.1.1",
    "require-all": "^3.0.0",
    "toposort-class": "^1.0.1"
  },
  "directories": {
    "example": "examples"
  },
  "engines": {
    "node": ">6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidesmiths/systemic.git"
  },
  "bugs": {
    "url": "https://github.com/guidesmiths/systemic/issues"
  },
  "homepage": "https://guidesmiths.github.io/systemic/",
  "husky": {
    "hooks": {
      "pre-commit": "npm run qa"
    }
  }
